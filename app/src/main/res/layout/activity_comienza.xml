<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".comienza">

    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/fondocomienza" />

    <Button
        android:id="@+id/btn_comienza_registrarse"
        android:layout_width="211dp"
        android:layout_height="0dp"
        android:layout_marginTop="450dp"
        android:layout_marginBottom="9dp"
        android:background="@drawable/registrarse"
        app:layout_constraintBottom_toTopOf="@+id/btn_comienza_iniciarsesion"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btn_comienza_iniciarsesion"
        android:layout_width="211dp"
        android:layout_height="0dp"
        android:layout_marginBottom="80dp"
        android:background="@drawable/iniciarsesion"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_comienza_registrarse" />


</android.support.constraint.ConstraintLayout>